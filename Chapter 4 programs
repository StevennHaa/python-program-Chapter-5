def main():
    num=eval(input("Enter a number to check if it is prime"))
    flag=False
    if num==1:
        print(num,"is a not prime number")
    elif num>1:
        for i in range (2,num):
            if (num%i)==0:
                flag=True
                break
    if flag:
        print(num,"is not a prime number")
    else:
        print(num,"is a prime number")
main()